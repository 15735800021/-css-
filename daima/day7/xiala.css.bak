*{margin:0;padding:0;}
li{list-style:none;}
a{text-decoration:none;color:#111;}
#na-list-section{
	width:150px;
	height:50px;
}
#na-list-section>a{
	width:150px;
	height:50px;
	display:block;
	text-align:center;
	line-height:50px;
	background:url(xiala.png) no-repeat 110px center;
	background-size:20%;
}
#na-list-section>a:hover{
	background-color:#ddd;
}
#na-list-section ul{
	display:none;
	padding:10px 0;
}
#na-list-section ul li{
	text-align:center;
	line-height:35px;
}
#na-list-section ul li:hover{
	background:#eee;
}
#na-list-section:hover ul{
	display:block;
}
form{width:800px;margin:auto;}
.ipt-box{
	width:798px;
	height:105px;
	border:1px solid #ddd;
	margin-top:64px;
}
.ipt-box span{
	width:170px;
	height:105px;
	margin-left:40px;
	float:left;
	line-height:105px;
	font-size:26px;
}
.thre span{
	letter-spacing:27px;
}
.fur span{
	letter-spacing:10px;
}
.ipt-box input{
	width:500px;
	height:105px;
	border:0;
	outline:none;
	float:left;
	font-size:28px;
	line-height:105px;
}
form p{
	text-align:right;font-size:28px;
}
form p a{color:#3388ff;}
.ipt-box .small{
	width:300px;
}
.ipt-box a{
	width:220px;
	height:92px;
	text-align:center;
	line-height:92px;
	margin:8px;
	float:right;
	background:#eee;
	font-size:22px;
}
.iagree{
	text-align:left;
	font-size:22px;
	margin:70px;
}
form>input{
	width:798px;
	height:105px;
	background-image:linear-gradient(to top,#e92121,#ee2222 70%);
	color:#fff;
	font-size:32px;
	border:0;
	outline:none;
}